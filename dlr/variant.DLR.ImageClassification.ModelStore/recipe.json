{
    "RecipeFormatVersion": "2020-01-25",
    "ComponentName": "variant.DLR.ImageClassification.ModelStore",
    "ComponentVersion": "{COMPONENT_VERSION}",
    "ComponentType": "aws.greengrass.generic",
    "ComponentDescription": "Downloads the resnet50_v1 image classification ML models to the device as artifacts.",
    "ComponentPublisher": "AWS",
    "Manifests": [
        {
            "Platform": {
                "os": "linux",
                "architecture": "arm"
            },
            "Lifecycle": {},
            "Artifacts": [
                {
                    "Uri": "s3://DLR-resnet50-armv7l-cpu-ImageClassification.zip",
                    "Unarchive": "ZIP",
                    "Permission": {
                        "Read": "OWNER",
                        "Execute": "NONE"
                    }
                }
            ]
        },
        {
            "Platform": {
                "os": "linux",
                "architecture": "amd64"
            },
            "Lifecycle": {},
            "Artifacts": [
                {
                    "Uri": "s3://DLR-resnet50-x86_64-cpu-ImageClassification.zip",
                    "Unarchive": "ZIP",
                    "Permission": {
                        "Read": "OWNER",
                        "Execute": "NONE"
                    }
                }
            ]
        },
        {
            "Platform": {
                "os": "linux",
                "architecture": "aarch64"
            },
            "Lifecycle": {},
            "Artifacts": [
                {
                    "Uri": "s3://DLR-resnet50-aarch64-cpu-ImageClassification.zip",
                    "Unarchive": "ZIP",
                    "Permission": {
                        "Read": "OWNER",
                        "Execute": "NONE"
                    }
                }
            ]
        },
        {
            "Platform": {
                "os": "windows",
                "architecture": "amd64"
            },
            "Lifecycle": {},
            "Artifacts": [
                {
                    "Uri": "s3://DLR-resnet50-win-cpu-ImageClassification.zip",
                    "Unarchive": "ZIP",
                    "Permission": {
                        "Read": "OWNER",
                        "Execute": "NONE"
                    }
                }
            ]
        }
    ],
    "Lifecycle": {}
}